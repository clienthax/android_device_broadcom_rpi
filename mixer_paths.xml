<ctl name="DL2 Left Equalizer"                       value="Band-pass 450-14500Hz"/>
<ctl name="DL2 Right Equalizer"                      value="Flat response"/>
<ctl name="Sidetone Equalizer"                       value="Flat response"/>
<ctl name="AMIC Equalizer"                           value="High-pass 0dB"/>
<ctl name="DMIC Equalizer"                           value="High-pass 0dB"/>
<ctl name="DL1 Tones Playback Volume"                value="0"/>
