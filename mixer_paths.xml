<ctl name="AUDUL Voice DL Volume"                    value="0"/>
<ctl name="SDT UL Volume"                            value="101"/>
<ctl name="SDT DL Volume"                            value="120"/>
<ctl name="DMIC1 UL Volume"                          value="120 120"/>
<ctl name="DMIC2 UL Volume"                          value="120 120"/>
<ctl name="DMIC3 UL Volume"                          value="120 120"/>
