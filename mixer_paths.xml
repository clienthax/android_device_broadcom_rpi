<ctl name="Sidetone Mixer Playback"                  value="1"/> <!-- value="On" -->
<ctl name="Capture Mixer Tones"                      value="0"/> <!-- value="Off" -->
<ctl name="Capture Mixer Voice Playback"             value="0"/> <!-- value="Off" -->
<ctl name="Capture Mixer Voice Capture"              value="0"/> <!-- value="Off" -->
<ctl name="Capture Mixer Media Playback"             value="0"/> <!-- value="Off" -->
<ctl name="Voice Capture Mixer Tones Playback"       value="0"/> <!-- value="Off" -->
<ctl name="Voice Capture Mixer Media Playback"       value="0"/> <!-- value="Off" -->
<ctl name="Voice Capture Mixer Capture"              value="0"/> <!-- value="Off" -->
<ctl name="DL2 Mixer Tones"                          value="0"/> <!-- value="Off" -->
<ctl name="DL2 Mixer Voice"                          value="0"/> <!-- value="Off" -->
<ctl name="DL2 Mixer Capture"                        value="0"/> <!-- value="Off" -->
<ctl name="DL1 Mixer Tones"                          value="0"/> <!-- value="Off" -->
<ctl name="DL1 Mixer Voice"                          value="0"/> <!-- value="Off" -->
<ctl name="DL1 Mixer Capture"                        value="0"/> <!-- value="Off" -->
<ctl name="MUX_VX1"                                  value="None"/>
<ctl name="MUX_VX0"                                  value="None"/>
<ctl name="MUX_UL11"                                 value="AMic0"/>
<ctl name="MUX_UL10"                                 value="AMic0"/>
<ctl name="MUX_UL07"                                 value="None"/>
<ctl name="MUX_UL06"                                 value="None"/>
<ctl name="MUX_UL05"                                 value="None"/>
