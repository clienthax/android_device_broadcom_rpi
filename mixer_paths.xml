<ctl name="BT UL Volume"                             value="120 120"/>
<ctl name="DL1 Mono Mixer"                           value="0"/> <!-- value="0" -->  
<ctl name="DL2 Mono Mixer"                           value="0"/> <!-- value="0" -->  
<ctl name="AUDUL Mono Mixer"                         value="0"/> <!-- value="0" -->  
<ctl name="DL1 MM_EXT Switch"                        value="0"/> <!-- value="0" -->  
<ctl name="DL1 BT_VX Switch"                         value="0"/> <!-- value="0" -->  
